{
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "buildCommand": "npm run build:prod",
    "outputDirectory": "dist",
    "framework": "vite",
    "installCommand": "npm install",
    "devCommand": "npm run dev",
    "build": {
      "env": {
        "SKIP_TYPE_CHECK": "true"
      }
    }
  }